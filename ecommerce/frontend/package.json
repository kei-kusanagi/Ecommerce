{
  "name": "python-webpack-boilerplate",
  "version": "0.0.5",
  "description": "Webpack boilerplate for Django & Flask",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "start": "webpack serve --config webpack/webpack.config.dev.js",
    "watch": "webpack --watch --config webpack/webpack.config.watch.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AccordBox/python-webpack-boilerplate"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend"
  ],
  "author": "Michael Yin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AccordBox/python-webpack-boilerplate/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.2",
    "autoprefixer": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "cssnano": "^5.1.12",
    "eslint": "^7.32.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.2.0",
    "sass": "~1.32.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "tailwindcss": "^3.1.4",
    "webpack": "^5.73.0",
    "webpack-assets-manifest": "^5.0.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "bootstrap": "^5.1.1",
    "core-js": "^3.16.1",
    "htmx.org": "^1.7.0"
  }
